{
  "name": "al-laham-fresh-meat",
  "description": "This is a monorepo for the Al Laham Fresh Meat project.",
  "registry": "https://registry.yarnpkg.com",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "contentful:login": "contentful login",
    "contentful:export": "contentful space export --space-id py1ip7ajrgdr --content-file contentful/models.json --skip-content --skip-webhooks --skip-roles",
    "contentful:import": "contentful space import --space-id py1ip7ajrgdr --content-file contentful/models.json",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "codegen": "graphql-codegen",
    "storybook": "turbo run storybook"
  },
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestjs/cli": "^10.4.7",
    "contentful-cli": "^3.5.5",
    "eslint": "^9.13.0",
    "graphql": "^16.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prisma": "^5.22.0",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@1.22.22"
}
